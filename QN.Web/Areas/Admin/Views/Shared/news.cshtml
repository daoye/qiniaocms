@model  IList<OnlineArticleDTO>

<div class="panel-padding no-padding-vr">
    @if (Model.Count > 0)
    {
        foreach (OnlineArticleDTO a in Model)
        {
            <div class="ticket">
                <a href="@a.Url" title="" class="ticket-title">@cut(a.Title, 20)</a>
            </div>
        }
    }
    else
    {
        <div class="padding-sm text-center">@lang("暂无最新动态。")</div>
    }
</div>