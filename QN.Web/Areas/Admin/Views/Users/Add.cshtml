@model user
@{
    ViewBag.active_nav = "useradd";

    ViewBag.Title = lang("添加新用户");
    Layout = "~/Areas/Admin/Views/Shared/_Layout.cshtml";
}

@section breadcrumb {
    <li><a href="@Url.Action("list")">@lang("用户管理")</a></li>
    <li class="active"><a href="@Url.Action("add")">@lang("添加新用户")</a></li>
}

@using (Html.BeginForm("add"))
{
    @Html.Partial("form", Model)
}

@section footer{
@scripts("~/scripts/jquery.unobtrusive-ajax.min.js",
            "~/scripts/jquery.validate.unobtrusive.js",
            "~/scripts/admin/validater.js")
}